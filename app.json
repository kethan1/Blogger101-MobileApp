{
  "expo": {
    "name": "Blogger101-MobileApp",
    "slug": "Blogger101-MobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ketzoomer.Blogger101MobileApp"
    },
    "android": {
      "package": "com.ketzoomer.Blogger101MobileApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
